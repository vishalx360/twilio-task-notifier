{
  "name": "fastify-typescript-starter",
  "version": "4.1.0",
  "description": "Fastify TypeScript starter repository",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "resetdb": "npx prisma migrate reset --preview-feature",
    "updatedb": "npx prisma db push",
    "studio": "npx prisma studio"
  },
  "author": "Matschik <mathieu.schimmerling@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "@joggr/fastify-prisma": "^1.2.2",
    "@prisma/client": "^5.8.1",
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^2.0.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "rimraf": "^5.0.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}